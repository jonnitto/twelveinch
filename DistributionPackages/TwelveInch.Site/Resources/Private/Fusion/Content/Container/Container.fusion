prototype(TwelveInch.Site:Content.Container) < prototype(Neos.Neos:Content) {
        templatePath = 'resource://Vendor.Site/Private/Templates/FusionObjects/Box.html'
        columnContent = Neos.Neos:ContentCollection
        columnContent {
                nodePath = 'column0'
        }

        renderer = afx`
        <div class="container ">
            <div class="column">
                    <fusion:render path="columnContent" />
            </div>
        </div>
        `
}
